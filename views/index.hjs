<!DOCTYPE html>
<html>
<head>
<title>{{ title }}</title>
<link rel='stylesheet' href='/styles/style.css' />
<script src='./socket.io/socket.io.js'></script>
<script src='http://code.jquery.com/jquery.js'></script>
</head>
<body>
<div id="container">
  <div id="columns">

    {{#tiles}}
      <a href="{{pageURL}}">
      <div class="tile">
        <h2 class="name">_{{ username }}</h2>
        <h3>
          {{ pageTitle }} 
        </h3>
        <span class="domain">{{ pageURL }}</span>
        <span class="timeStamp"></span>
      </div>
      </a>
    {{/tiles}}
  </div>
</div>
<script src='javascripts/socket.js'></script>
</body>
</html>
