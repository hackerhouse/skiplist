<!DOCTYPE html>
<html>
<head>
  <title>{{ title }}</title>
  <link href='http://fonts.googleapis.com/css?family=Roboto:400,100,300,300italic,100italic,400italic,900,900italic' rel='stylesheet' type='text/css'>
  <link rel='stylesheet' href='app/styles/css/style.css' />
  <script src="http://localhost:35729/livereload.js?snipver=1" type="text/javascript"></script>
  <script src='./socket.io/socket.io.js'></script>
  <script src='http://code.jquery.com/jquery.js'></script>
  <script src="/js/ui.js" type="text/javascript"></script>
</head>
<body>
  <div id="container">
    <div id="columns">

      {{#tiles}}
      <div class="tile">
       <a class="blockLink" href="{{pageURL}}" target="_blank"></a>
       <div class="options">
         <ul>
           <li><button class="option delete" value="{{_id}}">x</button></li>
         </ul>
       </div>
       <h2 class="name">_{{ username }}</h2>
       <h3>
        {{ pageTitle }} 
      </h3>
      <span class="domain">{{ prettyURL }}</span>
      <span class="timeStamp"></span>
    </div>
    {{/tiles}}
  </div>
</div>
<script src='js/socket.js'></script>
</body>
</html>
