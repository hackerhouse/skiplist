<div id="columns">
  {{#tiles}}
  <div class="tile">
    <a class="blockLink" href="{{pageURL}}" target="_blank"></a>
    <div class="options">
      <ul>
        <li><button class="option delete" value="{{_id}}">x</button></li>
      </ul>
    </div>
    <h2 class="name">_{{ username }}</h2>
    <h3>
      {{ pageTitle }} 
    </h3>
    <span class="domain">{{ prettyURL }}</span>
    <span class="timeStamp"></span>
  </div>
  {{/tiles}}
</div>
